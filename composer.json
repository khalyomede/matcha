{
	"name": "khalyomede/matcha",
	"description": "Unit test library that taste good.",
	"type": "library",
	"license": "MIT",
	"keywords": [
		"unit",
		"test",
		"testing",
		"tool"
	],
	"authors": [
		{
			"name": "khalyomede",
			"email": "khalyomede@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.2.0",
		"khalyomede/console-reporter": "0.*"
	},
	"provide": {
		"khalyomede/console-reporter": "0.*"
	},
	"autoload": {
		"psr-4": {
			"Khalyomede\\": "src"
		},
		"files": [
			"src/describe.php",
			"src/it.php",
			"src/expect.php",
			"src/run.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "6.*"
	},
	"config": {
		"bin-dir": "bin"
	}
}
