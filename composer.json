{
	"name": "khalyomede/matcha",
	"description": "Test tool for PHP",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "khalyomede",
			"email": "khalyomede@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.2.0"
	},
	"autoload": {
		"psr-4": {
			"Khalyomede\\": "src"
		},
		"files": [
			"src/describe.php",
			"src/it.php",
			"src/expect.php",
			"src/run.php"
		]
	},
	"require-dev": {
		"phpspec/phpspec": "5.*"
	},
	"config": {
		"bin-dir": "bin"
	}
}
